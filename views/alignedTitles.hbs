<style>
.left-of-term {
  text-align:right;
  white-space: nowrap"
}
.term {
  text-align:center;
  white-space: nowrap
}
.right-of-term {
  text-align:left;
  white-space: nowrap
}
.term-link{
  text-decoration: none;
}
</style>

<h1>Aligned Titles containing "{{term}}" in {{year}}</h1>

<table>
  {{#each results}}
  <tr>
    <td class="left-of-term"><a class="term-link" href="{{url}}">{{titleParts.[0]}}</a></td>
    <td class="term"><a class="term-link" href="{{url}}">{{titleParts.[1]}}</a></td>
    <td class="right-of-term"><a class="term-link" href="{{url}}">{{titleParts.[2]}}</a></td>
  </tr>
  {{/each}}
</table>
